@{
    ViewData["Title"] = "AddPersonToCity";
}

<h1>Lägga till Personer Till City</h1>

@if (ViewBag.Message != null)
{
    <h3>@ViewBag.Message</h3>
}



<form asp-action="AddPersonToCity"method="post">
    <div class="form-group">
        <label>Citys </label>
        <select asp-items="@ViewBag.Citys" name="Name"class="form-control"></select>
    </div>
    <div class="form-group">
        <label>Peoples  </label>
        <select asp-items="@ViewBag.People" name="id" class="form-control"></select>
    </div>
    <input type="submit" value="Lägg till"class="btn btn-primary"/>
</form>